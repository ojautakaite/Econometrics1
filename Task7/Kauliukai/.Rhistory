n=10^4
x1 <- replicate(n, sum(sample(1:6, 3, replace = TRUE))) #skaičiuojame sumas, kurias išridena pirmasis žaidėjas
x2 <- replicate(n, sum(sample(1:6, 2, replace = TRUE))) #skaičiuojame sumas, kurias išridena antrasis žaidėjas
x1_laimi<- sum((x1>x2)==TRUE) #apskačiuojame kiek kartų pirmasis žaidėjas išrideno didesnes sumas už antrąjį
ismetami=sum((x1==x2)==TRUE) #suskaičiuojame tuos bandymus, kur išridentos sumos yra lygios
prob<-x1_laimi/(n-ismetami) #Skaičiuojame tikimybe, laimėti pirmajam žaidėjui
prob
